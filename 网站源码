        <script>
                                    $.ajax({
                                        type: 'get',
                                        url: "https://ay.yimfk.com/index/resource/musicdetail.html?id=",
                                        dataType: "json",
                                        success: function (res) {

                                            var audio = [];

                                            if ("" != "")
                                            {
                                                audio.push({
                                                    title: '平台语音播报',
                                                    author: '平台提醒',
                                                    url: "",
                                                    cover: "/static/upload/3e67c4c4471199b5/1789c3f5adf310f4.png",
                                                });
                                            }

                                            if (res.code == 1)
                                            {
                                                audio.push({
                                                    name: res.song,
                                                    artist: res.singer,
                                                    url: res.musicLink,
                                                    cover: res.picture,
                                                });
                                            }

                                            if (audio.length > 0)
                                            {
                                                const ap = new APlayer({
                                                    container: document.getElementById('aplayer'),
                                                    fixed: true,
                                                    mini: true,
                                                    autoplay: true,
                                                    theme: '#FADFA3',
                                                    loop: 'all',
                                                    order: 'list',
                                                    preload: 'auto',
                                                    volume: 1,
                                                    mutex: true,
                                                    listFolded: false,
                                                    listMaxHeight: 90,
                                                    audio: audio
                                                });
                                            }

                                        }
                                    });
        </script>
